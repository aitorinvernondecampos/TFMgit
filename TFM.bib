	issn = {1467-842X},
	journal = {Australian \& New Zealand Journal of Statistics},
	keywords = {permutation tests, asymptotics, canonical analysis, classification, MANOVA, multivariate, resampling},
	language = {en},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/1467-842X.00285},
	number = {3},
	pages = {301--318},
	title = {Generalized discriminant analysis based on distances},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/1467-842X.00285},
