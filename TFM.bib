	note = {Publisher: Public Library of Science},
	number = {6},
	pages = {e66019},
	shorttitle = {Archaea and {Fungi} of the {Human} {Gut} {Microbiome}},
	title = {Archaea and {Fungi} of the {Human} {Gut} {Microbiome}: {Correlations} with {Diet} and {Bacterial} {Residents}},
	url = {https://journals.plos.org/plosone/article?id=10.1371/journal.pone.0066019},
	urldate = {2023-03-21},
