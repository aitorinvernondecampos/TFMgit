	journal = {American Journal of Human Genetics},
	month = oct,
	number = {4},
	pages = {567--575},
	pmcid = {PMC4596912},
	pmid = {26430802},
	title = {Alternative {Splicing} {QTLs} in {European} and {African} {Populations}},
